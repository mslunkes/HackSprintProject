<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SelectIcon">

    <TextView
        android:id="@+id/tv_title_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Select the category Icon"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/iv_Car"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_car"
        app:layout_constraintEnd_toStartOf="@+id/iv_house"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_icon" />

    <ImageView
        android:id="@+id/iv_house"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_home"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_icon"
        tools:layout_editor_absoluteX="200dp"/>

    <ImageView
        android:id="@+id/iv_clothes"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_clothes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/iv_house"
        app:layout_constraintTop_toBottomOf="@+id/tv_title_icon" />

    <ImageView
        android:id="@+id/iv_credid_card"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_credit_card"
        app:layout_constraintEnd_toStartOf="@+id/iv_house"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_Car" />

    <ImageView
        android:id="@+id/iv_electricity"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_electricity"
        app:layout_constraintTop_toBottomOf="@+id/iv_house"
        tools:layout_editor_absoluteX="200dp" />

    <ImageView
        android:id="@+id/iv_game_control"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_game_control"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/iv_house"
        app:layout_constraintTop_toBottomOf="@+id/iv_clothes" />

    <ImageView
        android:id="@+id/iv_gas_station"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_gas_station"
        app:layout_constraintEnd_toStartOf="@+id/iv_house"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_credid_card" />

    <ImageView
        android:id="@+id/iv_graphics"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_graphic"
        app:layout_constraintTop_toBottomOf="@+id/iv_electricity"
        tools:layout_editor_absoluteX="200dp" />

    <ImageView
        android:id="@+id/iv_key"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_key"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/iv_house"
        app:layout_constraintTop_toBottomOf="@+id/iv_game_control" />

    <ImageView
        android:id="@+id/iv_wifi"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_wifi"
        app:layout_constraintEnd_toStartOf="@+id/iv_house"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_gas_station" />

    <ImageView
        android:id="@+id/iv_water_drop"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_water_drop"
        app:layout_constraintTop_toBottomOf="@+id/iv_graphics"
        tools:layout_editor_absoluteX="200dp" />

    <ImageView
        android:id="@+id/iv_shopping_cart"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_shopping_cart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/iv_house"
        app:layout_constraintTop_toBottomOf="@+id/iv_key" />



    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:text="CREATE"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>